  <diagram id="iCFl6A7WpBeucsHr39uu" name="Example">
    <mxGraphModel dx="946" dy="646" grid="0" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="850" pageHeight="1100" math="1" shadow="0">
      <root>
        <mxCell id="KGHZM98poUtChthoUTiK-0" />
        <mxCell id="KGHZM98poUtChthoUTiK-1" parent="KGHZM98poUtChthoUTiK-0" />
        <mxCell id="KGHZM98poUtChthoUTiK-2" value="Linear Regression" style="text;strokeColor=none;fillColor=none;html=1;fontSize=24;fontStyle=1;verticalAlign=middle;align=center;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="286" y="19" width="279" height="40" as="geometry" />
        </mxCell>
        <mxCell id="KGHZM98poUtChthoUTiK-3" value="&lt;font style=&quot;font-size: 18px&quot;&gt;&lt;b&gt;Equation Model:&lt;/b&gt; $$f_{w,b}(x) = wx + b$$&lt;br&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="264" y="72" width="322" height="96" as="geometry" />
        </mxCell>
        <mxCell id="KGHZM98poUtChthoUTiK-4" value="&lt;font style=&quot;font-size: 18px&quot;&gt;&lt;b&gt;Loss Function:&lt;/b&gt; $$(f_{w,b}(x_i) - y_i)^2$$&lt;br&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="264" y="156" width="322" height="96" as="geometry" />
        </mxCell>
        <mxCell id="KGHZM98poUtChthoUTiK-5" value="&lt;font style=&quot;font-size: 18px&quot;&gt;&lt;b&gt;Gradient Descent (Optimization Routine):&lt;/b&gt; &lt;br&gt;1. Calculate Partial Derivatives&lt;br&gt;$$\frac{\partial l}{\partial w} = \frac{1}{N} \sum_{i=1}^{N} -2x_i(y_i - (wx_i + b))\\&lt;br&gt;\frac{\partial l}{\partial b} = \frac{1}{N} \sum_{i=1}^{N} -2(y_i - (wx_i + b))&lt;br&gt;$$&lt;br&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="219" y="413" width="412" height="96" as="geometry" />
        </mxCell>
        <mxCell id="KGHZM98poUtChthoUTiK-6" value="&lt;font style=&quot;font-size: 18px&quot;&gt;&lt;b&gt;Cost Function (Optimization Criterion):&lt;/b&gt; $$\frac{1}{N} \sum_{i...N}(f_{w,b}(x_i) - y_i)^2&lt;br&gt;$$&lt;br&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="250" y="255" width="351" height="96" as="geometry" />
        </mxCell>
        <mxCell id="KGHZM98poUtChthoUTiK-8" value="&lt;font style=&quot;font-size: 18px&quot;&gt;2. Initialize and solve&lt;br&gt;$$w \leftarrow w - \alpha \frac{\partial l}{\partial w}\\&lt;br&gt;b \leftarrow b - \alpha \frac{\partial l}{\partial b}&lt;br&gt;$$&lt;br&gt;&lt;/font&gt;" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="KGHZM98poUtChthoUTiK-1">
          <mxGeometry x="264" y="602" width="322" height="96" as="geometry" />
        </mxCell>
